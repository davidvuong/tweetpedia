{
  "name": "tweetpedia",
  "version": "0.0.1",
  "description": "Pulls tweets from Twitter then search the content on Wikipedia",
  "main": "src/server.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "start": "nodemon build/server.js --watch build/server.js",
    "build": "webpack --config webpack.config.js && webpack --config webpack.server.config.js",
    "watch:client": "webpack --progress --colors --config webpack.config.js --watch",
    "watch:server": "webpack --progress --colors --config webpack.server.config.js --watch",
    "debug:server": "nodemon --debug build/server.js --watch build/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/davidvuong/tweetpedia.git"
  },
  "author": "David Vuong <david.vuong256@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel": "^6.5.2",
    "babel-core": "^6.7.0",
    "babel-eslint": "^6.0.0-beta.5",
    "babel-jest": "^9.0.3",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "css-loader": "^0.23.1",
    "eslint": "2.2.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.1",
    "expect": "^1.15.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^2.4.5",
    "nock": "^7.2.2",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.1",
    "react-addons-test-utils": "^0.14.7",
    "redux-devtools": "^3.1.1",
    "redux-mock-store": "^1.0.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "es6-promise": "^3.1.2",
    "express": "^4.13.4",
    "isomorphic-fetch": "^2.2.1",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-dom": "^0.14.7",
    "react-overlays": "^0.6.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-bootstrap": "^0.20.1",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "twitter": "^1.2.5",
    "wtf_wikipedia": "^0.3.1"
  },
  "engines": {
    "node": "5.7.1"
  }
}
