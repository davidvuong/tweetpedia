{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "babel",
    "react"
  ],
  "rules": {
    "comma-dangle": [2, "never"],
    "react/prefer-stateless-function": 0,
    "react/prop-types": 0,

    "new-cap": 0,
    "no-console": 0,
    "arrow-body-style": 0
  }
}
